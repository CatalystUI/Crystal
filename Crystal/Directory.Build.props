<?xml version="1.0" encoding="utf-8" ?>
<Project>

    <!-- Project Properties -->
    <PropertyGroup>
        <!-- Build Properties -->
        <TargetFramework>net9.0</TargetFramework>
        <LangVersion>latest</LangVersion>

        <!-- Editor Properties -->
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>

        <!-- NativeAOT Declarations -->
        <PublishAot>true</PublishAot>
        <IsAotCompatible>true</IsAotCompatible>

        <!-- Trimming Declarations -->
        <PublishTrimmed>true</PublishTrimmed>
        <TrimMode>full</TrimMode>

        <!-- Assembly Signing -->
        <SignAssembly>true</SignAssembly>
        <AssemblyOriginatorKeyFile>$(MSBuildProjectName).snk</AssemblyOriginatorKeyFile>

        <!-- Use CatalystUI Optimizations -->
        <UseCatalystUIOptimizations>true</UseCatalystUIOptimizations>

        <!-- Common Metadata -->
        <Version>$(VersionPrefix)$(VersionSuffix)</Version>
        <Company>Visual Fire Development</Company>
        <Copyright>¬© 2024‚Äì2025 FireController#1847 ‚Äî All Rights Reserved</Copyright>
        <PackageProjectUrl>https://github.com/CatalystUI/Crystal</PackageProjectUrl>
        <RepositoryUrl>https://github.com/CatalystUI/Crystal</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <NeutralLanguage>en</NeutralLanguage>
        <PackageLicenseUrl>https://github.com/CatalystUI/Crystal/LICENSE</PackageLicenseUrl>
        <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
    </PropertyGroup>

    <!-- CatalystUI Optimizations -->
    <PropertyGroup Condition="'$(UseCatalystUIOptimizations)' == 'true'">
        <EventSourceSupport>false</EventSourceSupport>
        <HttpActivityPropagationSupport>false</HttpActivityPropagationSupport>
        <InvariantGlobalization>true</InvariantGlobalization>
        <MetadataUpdaterSupport>false</MetadataUpdaterSupport>
        <MetricsSupport>false</MetricsSupport>
        <StackTraceSupport>false</StackTraceSupport>
        <UseNativeHttpHandler>true</UseNativeHttpHandler>
        <UseSystemResourceKeys>true</UseSystemResourceKeys>
        <XmlResolverIsNetworkingEnabledByDefault>false</XmlResolverIsNetworkingEnabledByDefault>
    </PropertyGroup>

    <!-- Output Information -->
    <PropertyGroup>
        <OutputInfoFileExtension Condition="'$(OutputType)' == 'Exe' and $([MSBuild]::IsOSPlatform('Windows'))">.exe</OutputInfoFileExtension>
        <!--suppress CheckTagEmptyBody -->
        <OutputInfoFileExtension Condition="'$(OutputType)' == 'Exe' and !$([MSBuild]::IsOSPlatform('Windows'))"></OutputInfoFileExtension>
        <OutputInfoFileExtension Condition="'$(OutputType)' != 'Exe'">.dll</OutputInfoFileExtension>
        <OutputInfoAOT Condition="$(PublishAot)">üî• You know it.</OutputInfoAOT>
        <OutputInfoAOT Condition="!$(PublishAot)">üï∂Ô∏è Going for compat.</OutputInfoAOT>
        <OutputInfoSupportsRuntimeMod Condition="!$(MetadataUpdaterSupport)">false (‚úÖ Good! Supports NativeAOT)</OutputInfoSupportsRuntimeMod>
        <OutputInfoSupportsRuntimeMod Condition="$(MetadataUpdaterSupport)">true (‚ùå Bad! Does not support NativeAOT)</OutputInfoSupportsRuntimeMod>
        <NETCoreSdkVersion Condition="'$(NETCoreSdkVersion)' == ''">$([System.Environment]::GetEnvironmentVariable('DOTNET_SDK_VERSION'))</NETCoreSdkVersion>
    </PropertyGroup>

